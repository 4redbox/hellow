pipeline {
    agent any

    stages {
        stage('Compile') {
            steps {
                echo 'Hello World Compiled'
            }
        }
        stage('Test') {
            steps {
                echo 'Hello World Tested'
            }     
        }
        stage('Deploy') {
            steps {
                echo 'Hello World Deployed'
            }            
        }
    }
}
